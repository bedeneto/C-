using System;

namespace Projeto01
{
    internal class Program
    {
        static void Main(string[] args)
        {
            float TempC = 0;
            float TempF = 0;
                   
            Console.Write("Digite a temperatura em Farenheits: ");
            TempF = float.Parse(Console.ReadLine());

            TempC = 5 * ((TempF - 32) / 9);

            Console.WriteLine("A temperatura em graus Celsius é : " + TempC + "º");
            Console.ReadLine();

            
        }
    }
}
